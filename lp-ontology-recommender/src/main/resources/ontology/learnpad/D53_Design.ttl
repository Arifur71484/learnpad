      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object eo:LearningGoal ;
            sp:predicate rdf:type ;
            sp:subject spin:_this ;
          ]
          [
            sp:object spin:_this ;
            sp:predicate <http://ikm-group.ch/archiMEO/emo#learningGoalRepresentsLearningGoal> ;
            sp:subject spin:_this ;
