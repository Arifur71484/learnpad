      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object lpd:OperationalGoal ;
            sp:predicate rdf:type ;
            sp:subject spin:_this ;
          ]
          [
            sp:object spin:_this ;
            sp:predicate <http://ikm-group.ch/archiMEO/emo#operationalGoalRepresentsOperationalGoal> ;
            sp:subject spin:_this ;
          ]
